{
    "name": "TrainScrapingUtils",
    "version": "1.0",
    "description": "Does stuff with train scrapings",
    "manifest_version": 3,
    "icons": {
        "16": "resources/train_16.png",
        "48": "resources/train_48.png",
        "128": "resources/train_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://openrailwaymap.org/*",
                "https://openrailwaymap.org/*",
                "http://www.openrailwaymap.org/*",
                "https://www.openrailwaymap.org/*"
            ],
            "js": [
                "utils.js",
                "Api.js",
                "DnyLoader.js",
                "DnyRenderer.js",
                "icons.js",
                "openrailwaymaps_dny.js",
                "import.js"
            ],
            "css":[
                "openrailwaymaps_dny.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "resources/prod_00.png",
                "resources/prod_01.png",
                "resources/prod_04.png",
                "resources/prod_08.png",
                "resources/prod_16.png",
                "resources/prod_32.png"
            ],
            "matches": [
                "http://openrailwaymap.org/*",
                "https://openrailwaymap.org/*",
                "http://www.openrailwaymap.org/*",
                "https://www.openrailwaymap.org/*"
            ]
        }
    ]
}